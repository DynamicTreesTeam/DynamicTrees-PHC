{
  "multipart": [
    {   "when": { "south": "true" },
      "apply": { "model": "dynamictreesphc:vine_passionfruit" }
    },
    {   "when": { "west": "true" },
      "apply": { "model": "dynamictreesphc:vine_passionfruit", "y": 90 }
    },
    {   "when": { "north": "true" },
      "apply": { "model": "dynamictreesphc:vine_passionfruit", "y": 180 }
    },
    {   "when": { "east": "true" },
      "apply": { "model": "dynamictreesphc:vine_passionfruit", "y": 270 }
    },
    {   "when": { "up": "true" },
      "apply": { "model": "dynamictreesphc:vine_passionfruit_up" }
    },
    {   "when": { "up": "false", "north": "false", "west": "false", "south": "false", "east": "false"  },
      "apply": { "model": "dynamictreesphc:vine_passionfruit_up" }
    }
  ]
}